{# SERVICOS EN LA PORTADA #}
{% for item in values.servicios %}
    <div class="service-1 grid_4">
        <div class="tilte">
            <h2>{{item.Titulo}}</h2>
        </div>
        <div class="icon-wrapper"><img src="images/statics.png" alt=""></div>
        <div class="text"> {{item.Subtitulo}} </div>
        <div class="button-wrapper">
            <a href="{{item.Href.url}}"><span class="normal-button">{{values.LABELS.detalles}}</span></a>
        </div>
    </div>
{% endfor %}