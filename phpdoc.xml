<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <parser>
        <default-package-name>Tienda</default-package-name>
        <target>/doc</target>
    </parser>
    <transformer>
        <target>/doc/responsive</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>bin/Swift-5.0.0/*</ignore>
        <ignore>bin/Twig-1.15.0/*</ignore>
        <ignore>bin/pdf/*</ignore>
        <ignore>yaml/*</ignore>
        <ignore>config/*</ignore>
        <ignore>cron/*</ignore>
        <ignore>lib/*</ignore>
        <ignore>log/*</ignore>
        <ignore>plugins/*</ignore>
        <ignore>themes/*</ignore>
        <ignore>tmp/*</ignore>
    </files>
    <plugins>
        <plugin path="Core" />
    </plugins>
    <transformations>
        <template name="clean" />
    </transformations>
</phpdocumentor>
